ci.t <- function(x,alpha=0.05) {
n <- length(smp)
m <- mean(x)
s <- sd(x)
t <- qt(1-(alpha/2),df=n-1)
ll <- m - t * (s / sqrt(n))
ul <- m + t * (s / sqrt(n))
ci <- c(1-alpha, ll, m, ul)
names(ci) <- c("Confi","LO","UP")
return(ci)
}
smp <- c(520,489,481,512,515,542,520,518,527,526)
ci.t(smp)
ci.t(smp,0.1)
names(ci) <- c("Confi","LO","mean","UP")
