set.seed(9)
n <- 10
x <- 1:100
y <- seq(-3,3,by=0.01)
smps <- matrix(rnorm(n * length(x)), ncol = n)
xbar <- apply(smps, 1, mean)
se <- 1 / sqrt(10)
alpha <- 0.05
z <- qnorm(1 - alpha/2)
ll <- xbar - z * se
ul <- xbar + z * se
plot(y,type="n")
abline(h=0,col="red")
1.c <- rep(NA, length(x))
l.c <- rep(NA, length(x))
l.c <- ifelse(ll * ul >0 , "red", "black")
arrows(1:length(x), ll, 1:length(x),ul, code = 3)
plot(y,type="n",xlim=c(1,100), ylim=c(1.5,1.5))
abline(h=0,col="red")
